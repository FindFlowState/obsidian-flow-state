{
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["ES2020", "DOM"],
      "module": "ESNext",
      "moduleResolution": "Bundler",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "noEmit": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "types": ["node", "obsidian"]
    },
    "include": ["src/**/*.ts"]
  }